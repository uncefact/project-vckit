    "@veramo/did-resolver": "5.5.3",
    "@veramo/key-manager": "5.5.3",
    "@veramo/kms-local": "5.5.3",
    "@veramo/message-handler": "5.5.3",
    "@veramo/remote-client": "5.5.3",
    "@veramo/selective-disclosure": "5.5.3",
    "@veramo/url-handler": "5.5.3",
    "@veramo/utils": "5.5.3",
    "blessed": "^0.1.81",
    "commander": "^11.1.0",
    "console-table-printer": "^2.12.0",
    "cors": "^2.8.5",
    "credential-ld": "link:.tmp_npm/packages/credential-ld",
    "credential-w3c": "link:.tmp_npm/packages/credential-w3c",
    "cross-fetch": "^4.0.0",
    "date-fns": "^3.1.0",
    "debug": "^4.3.4",
    "did-resolver": "^4.1.0",
    "dotenv": "^16.3.1",
    "ethr-did-resolver": "^10.1.2",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "fuzzy": "^0.1.3",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "json-schema": "^0.4.0",
    "json5": "^2.2.3",
    "jsonpointer": "^5.0.1",
    "oas-resolver": "^2.5.6",
    "openapi-types": "^12.1.3",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.11.3",
    "qrcode-terminal": "^0.12.0",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "ts-json-schema-generator": "^1.5.0",
    "typeorm": "^0.3.19",
    "url-parse": "^1.5.10",
    "web-did-resolver": "^2.0.27",
    "ws": "^8.16.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/inquirer": "9.0.7",
    "@types/node-fetch": "2.6.10",
    "@types/passport-http-bearer": "1.0.41",
    "@types/url-parse": "1.4.11",
    "@types/ws": "8.5.10",
    "typescript": "5.1.6"
  },
  "files": [
    "bin/**/*",
    "build/**/*",
    "default/**/*",
    "src/**/*",
    "views/**/*",
    "README.md",
    "LICENSE"
